{
  "builds": [
    {
      "src": "ShayakAI.wsgi.py",
      "use": "@vercel/python",
      "config": {
        "distDir": "build",
        "maxLambdaSize": "50mb",
        "runtime": "python3.13"
      }
    }
  ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "ShayakAI.wsgi.py"
        }
    ],
}